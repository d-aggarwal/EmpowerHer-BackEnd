{
  "name": "empowerher-backend",
  "version": "1.0.0",
  "description": "Business Enterperneur",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev-express\" \"npm run start-model1\" \"npm run start-model2\" \"npm run start-model3\" \"npm run start-model4\"",
    "dev-express": "nodemon -r dotenv/config --experimental-json-modules src/index.js",
    "start-model1": "python3 modelai/businessplan.py", 
    "start-model2": "python modelai/marketingstrategy.py", 
    "start-model3": "cd modelai && python3 namegenerator.py",
    "start-model4": "cd modelai && python timelinegenerate.py"
  },
